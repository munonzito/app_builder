# Firebase Configuration
# Get these values from your Firebase Console > Project Settings

# iOS Firebase API Key
FIREBASE_IOS_API_KEY=your-ios-api-key

# Android Firebase API Key
FIREBASE_ANDROID_API_KEY=your-android-api-key

# Firebase Auth Domain (e.g., your-project.firebaseapp.com)
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com

# Firebase Project ID
FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket (e.g., your-project.firebasestorage.app)
FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app

# Firebase Messaging Sender ID
FIREBASE_MESSAGING_SENDER_ID=your-sender-id

# iOS Firebase App ID
FIREBASE_IOS_APP_ID=your-ios-app-id

# Android Firebase App ID
FIREBASE_ANDROID_APP_ID=your-android-app-id
